{
  "name": "vueify-example",
  "version": "1.0.0",
  "description": "Vueify transform for Browserify example",
  "main": "index.js",
  "scripts": {
    "dev": "watchify -vd -p browserify-hmr -t vueify -e src/main.js -o build/build2.js ",
    "build": "browserify -t vueify -e src/main.js | uglifyjs -c warnings=false -m > build/build.js"
  },
  "author": "Evan You",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "babel-runtime": "^6.0.14",
    "browserify-hmr": "^0.3.1",
    "http-server": "^0.8.5",
    "jade": "^1.11.0",
    "stylus": "^0.52.4",
    "uglify-js": "^2.5.0",
    "vue-hot-reload-api": "^1.2.1",
    "vueify": "^6.0.0",
    "vueify-insert-css": "^1.0.0",
    "watchify": "^3.4.0"
  },
  "dependencies": {
    "stylus": "^0.52.4",
    "vue": "^1.0.0",
    "vue-resource": "^0.1.17"
  }
}
